    "version": "0.2.0",
    "configurations": [
        {
            "type": "emmylua_launch",
            "request": "launch",
            "name": "luajit-bin",
            "program": "${workspaceRoot}/luajit-bin.exe",
            "captureStd": true,
            "captureOutputDebugString": false,
            "workingDir": "${workspaceRoot}",
            "arguments": ["lua/test_random.lua"]
        },
        {
            "type": "emmylua_launch",
            "request": "launch",
            "name": "Attach by launch nginx",
            "program": "${workspaceRoot}/nginx.exe",
            "captureStd": true,
            "workingDir": "${workspaceRoot}",
            "arguments": []
        }
    ]
}