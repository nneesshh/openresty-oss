# JP$(OBJ_EXT) depends on .h and .exh files not .c files - but all written by enc2xs
JP.c : JP.xs  cp_00_t.c eu_01_t.c ji_02_t.c ji_03_t.c ji_04_t.c ma_05_t.c sh_06_t.c

JP$(OBJ_EXT) : JP.c

